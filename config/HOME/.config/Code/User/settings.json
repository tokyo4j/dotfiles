{
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "editor.detectIndentation": false,
  "editor.fastScrollSensitivity": 5,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.minimap.enabled": false,
  "editor.stickyScroll.enabled": true,
  "git.confirmSync": false,
  "security.workspace.trust.enabled": false,
  "window.menuBarVisibility": "hidden",
  "window.titleBarStyle": "custom",
  "window.commandCenter": false,
  "workbench.startupEditor": "none",
  "workbench.localHistory.enabled": false,
  "workbench.colorTheme": "Monokai",
  "workbench.colorCustomizations": {
    "[Monokai]": {
      "editorInlayHint.foreground": "#868686f0",
      "editorInlayHint.background": "#18181800",
      "editorInlayHint.typeForeground": "#486148f0",
      "editorInlayHint.parameterForeground": "#486148f0",
      "editorStickyScroll.background": "#4d4d4d",
      "editorStickyScrollHover.background": "#4d4d4d",
      "tab.activeBackground": "#2d3f59",
      "window.activeBorder": "#ff0000"
    }
  },
  "extensions.ignoreRecommendations": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.defaultProfile.linux": "fish",
  "terminal.integrated.env.linux": {
    "EDITOR": "code --wait"
  },
  "terminal.integrated.shellIntegration.decorationsEnabled": "never",
  "terminal.integrated.altClickMovesCursor": false,
  "terminal.integrated.commandsToSkipShell": [
    "workbench.action.toggleSidebarVisibility",
    "workbench.action.toggleAuxiliaryBar"
  ],
  "diffEditor.ignoreTrimWhitespace": false,
  "explorer.sortOrder": "type",
  "window.zoomLevel": 1,
  "svelte.enable-ts-plugin": true,
  "cSpell.enabled": false,
  "hediet.vscode-drawio.resizeImages": null,
  "hediet.vscode-drawio.theme": "min",
  "github.gitAuthentication": false,
  "git.terminalAuthentication": false,
  "C_Cpp.default.cStandard": "gnu17",
  "C_Cpp.default.cppStandard": "gnu++17",
  "C_Cpp.autoAddFileAssociations": false,
  "C_Cpp.default.compilerPath": "/usr/bin/clang",
  "C_Cpp.intelliSenseEngine": "disabled",
  "clangd.arguments": [
    "--function-arg-placeholders=0",
    "--header-insertion=never"
  ],
  "cscope-code.auto": false,
  "editor.inlayHints.enabled": "offUnlessPressed",
  "rust-analyzer.server.path": "/usr/bin/rust-analyzer",
  "zig.path": "/usr/bin/zig",
  "zig.zls.path": "/usr/bin/zls",
  "zig.initialSetupDone": true,
  "zig.zls.enableArgumentPlaceholders": false,
  "sqltools.autoOpenSessionFiles": false,
  "remote.SSH.useLocalServer": false,
  "mesonbuild.configureOnOpen": false,
  "mesonbuild.downloadLanguageServer": false,
  "mesonbuild.languageServer": null,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[c]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.tabSize": 4
  },
  "[cpp]": {
    "editor.defaultFormatter": "xaver.clang-format",
    "editor.tabSize": 4
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.tabSize": 2
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "[php]": {
    "editor.defaultFormatter": "bmewburn.vscode-intelephense-client"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "off"
  },
  "[zig]": {
    "editor.tabSize": 4
  },
  "[glsl]": {
    "editor.defaultFormatter": "xaver.clang-format"
  }
}
